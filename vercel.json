{
  "version": 2,
  "routes": [
    {
      "src": "/gamedevteam",
      "dest": "/api/_middleware"
    },
    {
      "src": "/nlpteam",
      "dest": "/api/_middleware"
    },
    {
      "src": "/aiteam",
      "dest": "/api/_middleware"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
} 